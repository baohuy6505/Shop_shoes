{
  "name": "shop-shoes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "sass": "sass public/scss/main.scss public/stylesheets/main.css",
    "sass:watch": "sass --watch public/scss/main.scss public/stylesheets/main.css",
    "devall": "concurrently \"npm run sass:watch\" \"npm run dev\""
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.2",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.1",
    "morgan": "~1.9.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "sass": "^1.93.3"
  }
}
